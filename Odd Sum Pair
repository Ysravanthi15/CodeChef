//Odd Sum Pair
class Solution {
    public String checkOddPairs(int a, int b, int c) {
        // write your code here
        int oddCount = 0;
        if (a % 2 != 0) oddCount++;
        if (b % 2 != 0) oddCount++;
        if (c % 2 != 0) oddCount++;

        if (oddCount == 1 || oddCount == 2)
            return "YES";
        else
            return "NO";
    }
}
