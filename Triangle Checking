//Triangle Checking
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int A=sc.nextInt();
		int B=sc.nextInt();
		int C=sc.nextInt();
		int res=A+B;
		int res1=B+C;
		int res2=A+C;
		if(res>C && res1>A && res2>B){
		    System.out.println("YES");
		}
		else{
		    System.out.println("NO");
		}
}
}
