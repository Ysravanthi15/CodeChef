//Chef and Socks
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int A=sc.nextInt();
		int X=sc.nextInt();
		int Y=sc.nextInt();
		int res=X+Y;
		if(A<=res){
		    System.out.println("YES");
		    
		}
		else{
		    System.out.println("NO");
		}
		

	}
}
