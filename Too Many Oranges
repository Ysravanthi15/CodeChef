//Too Many Oranges
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		   int N=sc.nextInt();
		   int K=sc.nextInt();
		   int min=10*N;
		   int max=12*N;
		   if(K>=min && K <=max){
		       System.out.println("YES");
		   }
		   else{
		       System.out.println("NO");
		   }
		   
		}

	}
}
