//number reverse
public class Main {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Main. */
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sign=(n<0)?-1:1;
        n=Math.abs(n);
        int rev=0;
        while(n>0){
            rev=rev*10+(n%10);
            n=n/10;
        }
        System.out.print(sign*rev);  
    }
}
