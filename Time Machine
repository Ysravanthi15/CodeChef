//Time Machine
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		if(T==2050){
		    System.out.println("YES");
		}
		else{
		    int res=2050-T;
		    if(res<=25){
		        System.out.println("YES");
		    }
		    else{
		        System.out.println("NO");
		    }
		}

	}
}
