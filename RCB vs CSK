//RCB vs CSK
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int X=sc.nextInt();
		int Y=sc.nextInt();
		int res=X-Y;
		if(res>=18){
		    System.out.println("RCB");
		}
		else{
		    System.out.println("CSK");
		}

	}
}
