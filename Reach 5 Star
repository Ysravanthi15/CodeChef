//Reach 5 Star
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int X=sc.nextInt();
		int Y=sc.nextInt();
		int res=X+Y;
		if(res>=2000){
		    System.out.println("YES");
		}
		else{
		    System.out.println("NO");
		}

	}
}
