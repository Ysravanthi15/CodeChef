//Summer Heat
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		   int X=sc.nextInt();
		   int Y=sc.nextInt();
		   int Z=sc.nextInt();
		   int W=sc.nextInt();
		   long res=(long)Z/X;
		   long res1=(long)W/Y;
		   System.out.println(res+res1);
		}

	}
