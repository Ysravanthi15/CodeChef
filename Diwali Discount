//Diwali Discount
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int A=sc.nextInt();
		int B=sc.nextInt();
		if(A>B){
		    System.out.println(A-B);
		}
		else{
		    System.out.println("0");
		}
		

	}
}
