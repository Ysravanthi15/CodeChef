//Move Grid
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int A=sc.nextInt();
		int B=sc.nextInt();
		int C=sc.nextInt();
		int D=sc.nextInt();
		int res=A-C;
		int res1=B-D;
		System.out.print(res+" ");
		System.out.print(res1);
	}
}
